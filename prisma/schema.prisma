

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}
model User {
  id String @id @map("_id") @default(uuid())
  firstname String 
  lastname String 
  email String @unique
  password String 
}

model Task {
    id String @id @map("_id") @default(uuid())
    title String
    description String
    status TaskStatus

}

enum TaskStatus {
  PENDING
  STARTING
  FINISHING
}

